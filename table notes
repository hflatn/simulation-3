create table userbase (
id serial primary key unique,
user_id varchar(200) unique,
first_name varchar(40),
last_name varchar(40),
gender varchar(40),
hair_color varchar(40),
eye_color varchar(40),
hobby varchar(40),
birthday_day INT2,
birthday_month INT2,
birthday_year INT4
)